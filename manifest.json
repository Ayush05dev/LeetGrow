{
    "manifest_version": 3,
    "name": "LeetGrow",
    "version": "1.0",
    "description": "Generate Test Cases for LeetCode Problems and Analyze user's code to detect potential bugs.",
    "permissions": ["activeTab", "scripting","tabs"],
    "host_permissions": ["https://leetcode.com/problems/*"],
    "background": {
      "service_worker": "background.js",
      "type":"module"
    },
    "action": {
      "default_icon": {
        "16": "icon2.png",
        "48": "icon2.png",
        "128": "icon2.png"
      }
    }
    
  }



  